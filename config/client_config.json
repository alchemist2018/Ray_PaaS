{
  "inbounds": [    
    {
    "port": 8080,
    "listen": "127.0.0.1",
    "protocol": "http",
    "settings": {
      "timeout": 0
      }
    },
    {
    "port": 1080,
    "listen": "127.0.0.1",
    "protocol": "socks",
